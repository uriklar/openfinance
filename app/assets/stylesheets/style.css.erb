.tooltip {
  -webkit-box-shadow: 2px 2px 3px 3px #666;
box-shadow: 2px 2px 3px 3px #666;
  position: absolute;
  top: 100px;
  left: 100px;
  -moz-border-radius:5px;
  border-radius: 5px;
  border: 2px solid #000;
  /*background: #222222;*/ 
  background: #F8F7F7;
  opacity: .9;
  color: #eeeeee; 
  color: black;
  padding: 10px;
  width: 250px;
  font-size: 12px;
  z-index: 10;
  direction: rtl;
}

.tooltip .title {
  display: block;
  text-align: center;
  /*font-weight:bold;*/
  font-size: 15px;
}

.tooltip .budget {
  position:relative;
  left: -130px;
  display: inline-block;
  font-weight:bold;
  font-size: 18px;
}

.tooltip .percent {
  display: inline-block;
  position: relative;
  right: -80px;
  font-weight:bold;
  font-size: 18px;
  color: green;
  direction: ltr;
}

.tooltip .percent_title {
  display: inline-block;
  position: relative;
  right: -23px;
  font-weight:bold;
  font-size: 11px;
  font-weight: lighter;
  text-decoration: underline;
  direction: ltr;
}

.tooltip .budget_title {
  position:relative;
  left: -150px;
  display: inline-block;
  text-decoration: underline;
  font-weight: lighter;
  font-size: 11px;
}

